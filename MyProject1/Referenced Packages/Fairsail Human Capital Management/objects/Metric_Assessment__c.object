if(  Score__c &lt;  Required__c, $Label.fhcm2__Process_Metrics_Not_Achieved,  
if(  Score__c &lt;  Required__c + blankvalue(Margin__c,0), $Label.fhcm2__Process_Metrics_Partly_Achieved, $Label.fhcm2__Process_Metrics_Achieved )
)
)</formula>
